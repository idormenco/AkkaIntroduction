<Window x:Class="Akka.Net.AdvancedExample.Printer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="Auto" Width="Auto">
    <Grid Name="GridLayout">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="100"/>
        </Grid.ColumnDefinitions>
        <DockPanel Name="Panel" Grid.Column="0" Dock="Top"></DockPanel>
        <StackPanel Grid.Column="1" Orientation="Vertical">
            <Button Name="KillButton" Click="KillButton_OnClick"  FontSize="25" Margin="0,50,0,0">Catch</Button>
        </StackPanel>
    </Grid>
</Window>
